{
    "defaultProfile": "{preferrable-guid}",
    "copyOnSelect": false,
    "copyFormatting": "none",
    "largePasteWarning": true,
    "multiLinePasteWarning": false,
    "confirmCloseAllTabs": false,
    "initialCols": 100,
    "initialRows": 25,
    "initialPosition": "250,150",

    "profiles": {
        "list": [
            {
                "name": "Git Bash",
                "commandline": "%PROGRAMFILES%/git/usr/bin/bash.exe -i -l",
                "startingDirectory": "%USERPROFILE%\\Documents\\Projects",
                "guid": "{git-bash-guid}",
                "colorScheme": "GitBash",
                "icon": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\GitBash_Icon.png",
                "backgroundImage": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\GitBash_BG.png",
                "backgroundImageOpacity" : 0.3,
                "backgroundImageStretchMode" : "uniform",
                "backgroundImageAlignment": "bottomRight",
                "useAcrylic": true
            },
            {
                "name": "WSL",
                "commandline": "wsl.exe -d Ubuntu",
                "startingDirectory": "/mnt/c/Users/<username>/Documents/Projects",
                "guid": "{wsl-guid}",
                "icon": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\Ubuntu_Icon.png",
                "colorScheme": "UbuntuLegit"
            },
            {
                "name": "PowerShell",
                "commandline": "powershell.exe",
                "startingDirectory": "C:\\Users\\<username>\\Documents\\Projects",
                "guid": "{powershell-guid}",
                "icon": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\PowerShell_Icon.png",
                "backgroundImage": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\PowerShell_BG.png",
                "backgroundImageOpacity": 0.2,
                "backgroundImageStretchMode": "uniform",
                "backgroundImageAlignment": "bottomRight",
                "colorScheme": "PowerShellTom",
                "opacity": 80,
                "useAcrylic": true
            },
            {
                "name": "Command Prompt",
                "commandline": "cmd.exe",
                "startingDirectory": "C:\\Users\\<username>\\Documents\\Projects",
                "guid": "{cmd-guid}",
                "icon": "C:\\Users\\<username>\\Documents\\Projects\\Personal\\Git\\useful-tools\\WindowsTerminal\\images\\CMD_Icon.png"
            }
        ]
    },

    "keybindings": [
        { "command": "copy", "keys": "ctrl+c" },
        { "command": "paste", "keys": "ctrl+v" },
        { "command": "newTab", "keys": "ctrl+t" },

        { "command": "find", "keys": "ctrl+shift+f" },
        { "command": { "action": "findMatch", "direction": "next" } },
        { "command": { "action": "findMatch", "direction": "prev" } }
    ],

